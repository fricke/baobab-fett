{
  "manifest_version": 2,
  "name": "Baobab Fett",
  "description": "Adds Baobab debugging tools to the Chrome Developer Tools a little React",
  "version": "0.0.1",
  "minimum_chrome_version": "43",
  "devtools_page": "devtools.html",
  "background": {
      "scripts": [
          "background.js"
      ]
  },
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
  ]
}